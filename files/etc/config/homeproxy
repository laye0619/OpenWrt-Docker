config dns_server 'dns_server_okx'
	option label 'dns_server_okx'
	option enabled '1'
	option address 'tcp://8.8.8.8'
	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option outbound 'AWS'

config dns_server 'google'
	option label 'google'
	option enabled '1'
	option address 'https://dns.google/dns-query'
	option address_resolver 'default-dns'
	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option outbound 'AWS'
	option client_subnet '123.123.123.123'

config ruleset 'cn_ip'
	option label 'cn_ip'
	option enabled '1'
	option type 'remote'
	option format 'binary'
	option url 'https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs'
